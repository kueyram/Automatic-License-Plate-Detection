    "# Import necessary libraries\n",
    "import os\n",
    "import xml.etree.ElementTree as ET  # For parsing XML files\n",
    "import cv2  # For image processing\n",
    "\n",
    "# Function to convert XML annotations to YOLO format\n",
    "def convert_to_yolo_format(xml_file, img_width, img_height):\n",
    "    \"\"\"\n",
    "    Converts a single XML annotation file to YOLO format.\n",
    "\n",
    "    Args:\n",
    "        xml_file (str): Path to the XML file.\n",
    "        img_width (int): Width of the image.\n",
    "        img_height (int): Height of the image.\n",
    "\n",
    "    Returns:\n",
    "        list: YOLO annotations as a list of strings.\n",
    "    \"\"\"\n",
    "    tree = ET.parse(xml_file)  # Parse the XML file\n",
    "    root = tree.getroot()  # Get the root element of the XML\n",
    "    yolo_annotations = []  # List to store YOLO formatted data\n",
    "\n",
    "    # Iterate through all objects (e.g., license plates) in the annotation\n",
    "    for obj in root.findall(\"object\"):\n",
    "        class_id = 0  # Assuming a single class (license plate)\n",
    "\n",
    "        # Get the bounding box (xmin, ymin, xmax, ymax)\n",
    "        bndbox = obj.find(\"bndbox\")\n",
    "        xmin = int(bndbox.find(\"xmin\").text)\n",
    "        ymin = int(bndbox.find(\"ymin\").text)\n",
    "        xmax = int(bndbox.find(\"xmax\").text)\n",
    "        ymax = int(bndbox.find(\"ymax\").text)\n",
    "\n",
    "        # Convert bounding box to YOLO format (normalized)\n",
    "        x_center = ((xmin + xmax) / 2) / img_width\n",
    "        y_center = ((ymin + ymax) / 2) / img_height\n",
    "        width = (xmax - xmin) / img_width\n",
    "        height = (ymax - ymin) / img_height\n",
    "\n",
    "        # Format the data as: <class_id> <x_center> <y_center> <width> <height>\n",
    "        yolo_annotations.append(f\"{class_id} {x_center} {y_center} {width} {height}\\n\")\n",
    "    \n",
    "    return yolo_annotations\n",
    "\n",
    "# Function to convert all XML annotations in a directory\n",
    "def convert_annotations(annotations_dir, output_dir, img_dir):\n",
    "    \"\"\"\n",
    "    Converts all XML annotations in a directory to YOLO format.\n",
    "\n",
    "    Args:\n",
    "        annotations_dir (str): Path to the directory containing XML files.\n",
    "        output_dir (str): Path to save the YOLO annotations.\n",
    "        img_dir (str): Path to the directory containing corresponding images.\n",
    "    \"\"\"\n",
    "    os.makedirs(output_dir, exist_ok=True)  # Ensure output directory exists\n",
    "\n",
    "    # Loop through all files in the annotations directory\n",
    "    for file in os.listdir(annotations_dir):\n",
    "        if file.endswith(\".xml\"):  # Process only XML files\n",
    "            xml_path = os.path.join(annotations_dir, file)  # Full path to XML file\n",
    "            img_path = os.path.join(img_dir, file.replace(\".xml\", \".jpg\"))  # Corresponding image file\n",
    "            \n",
    "            # Get image dimensions (width, height)\n",
    "            img = cv2.imread(img_path)\n",
    "            img_height, img_width, _ = img.shape\n",
    "            \n",
    "            # Convert XML to YOLO format\n",
    "            yolo_format = convert_to_yolo_format(xml_path, img_width, img_height)\n",
    "            \n",
    "            # Save YOLO annotation to a .txt file\n",
    "            output_file = os.path.join(output_dir, file.replace(\".xml\", \".txt\"))\n",
    "            with open(output_file, \"w\") as f:\n",
    "                f.writelines(yolo_format)\n",
    "\n",
    "    print(\"Annotations converted successfully.\")\n"
