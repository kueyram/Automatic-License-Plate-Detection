    "import os\n",
    "import xml.etree.ElementTree as ET\n",
    "from PIL import Image\n",
    "\n",
    "# Define directories for annotations, images, and YOLO labels\n",
    "annotations_dir = os.path.join('..', 'data', 'annotations')  # Directory containing XML files\n",
    "images_dir = os.path.join('..', 'data', 'images')  # Directory containing image files\n",
    "yolo_labels_dir = os.path.join('..', 'data', 'labels')  # Directory to save YOLO labels\n",
    "yolov3_files_dir = os.path.join('..', 'yolov3_files')  # Directory containing YOLO config files\n",
    "\n",
    "# Ensure the directories exist\n",
    "if not os.path.exists(annotations_dir):\n",
    "    print(f\"Error: {annotations_dir} does not exist.\")\n",
    "    exit()\n",
    "\n",
    "if not os.path.exists(images_dir):\n",
    "    print(f\"Error: {images_dir} does not exist.\")\n",
    "    exit()\n",
    "\n",
    "if not os.path.exists(yolo_labels_dir):\n",
    "    os.makedirs(yolo_labels_dir)  # Create the labels directory if it doesn't exist\n",
    "\n",
    "\n",
    "# Paths for YOLO configuration files\n",
    "cfg_file = os.path.join(yolov3_files_dir, 'yolov3.cfg')\n",
    "weights_file = os.path.join(yolov3_files_dir, 'yolov3.weights')\n",
    "names_file = os.path.join(yolov3_files_dir, 'coco.names')    \n",
    "    \n",
    "    \n",
    "# Function to convert a single XML annotation to YOLO format\n",
    "def convert_to_yolo_format(xml_file, img_shape):\n",
    "    tree = ET.parse(xml_file)  # Parse the XML file\n",
    "    root = tree.getroot()  # Get the root of the XML tree\n",
    "    yolo_format = []  # List to store YOLO formatted annotations\n",
    "\n",
    "    # Image width and height from the shape\n",
    "    img_width, img_height = img_shape\n",
    "\n",
    "    # Iterate over each object in the XML annotation\n",
    "    for member in root.findall('object'):\n",
    "        class_name = member.find('name').text  # Extract the object class name\n",
    "        class_id = 0  # For simplicity, class_id is set to 0 for license plates\n",
    "\n",
    "        # Extract bounding box coordinates: xmin, ymin, xmax, ymax\n",
    "        bndbox = member.find('bndbox')\n",
    "        xmin = int(bndbox.find('xmin').text)\n",
    "        ymin = int(bndbox.find('ymin').text)\n",
    "        xmax = int(bndbox.find('xmax').text)\n",
    "        ymax = int(bndbox.find('ymax').text)\n",
    "\n",
    "        # Convert bounding box coordinates to YOLO format (normalized)\n",
    "        x_center = (xmin + xmax) / 2 / img_width\n",
    "        y_center = (ymin + ymax) / 2 / img_height\n",
    "        width = (xmax - xmin) / img_width\n",
    "        height = (ymax - ymin) / img_height\n",
    "\n",
    "        # Append the YOLO formatted data for this object to the list\n",
    "        yolo_format.append(f\"{class_id} {x_center} {y_center} {width} {height}\\n\")\n",
    "\n",
    "    return yolo_format\n",
    "\n",
    "# Loop through all XML files in the annotations directory\n",
    "for xml_file in os.listdir(annotations_dir):\n",
    "    if xml_file.endswith('.xml'):  # Process only XML files\n",
    "        xml_path = os.path.join(annotations_dir, xml_file)  # Full path to the XML file\n",
    "\n",
    "        # Check for the corresponding image file (assumes PNG format)\n",
    "        img_path = os.path.join(images_dir, xml_file.replace('.xml', '.png'))\n",
    "\n",
    "        # If the image file does not exist, print a warning and skip this annotation\n",
    "        if not os.path.exists(img_path):\n",
    "            print(f\"Warning: Image file {img_path} does not exist. Skipping...\")\n",
    "            continue\n",
    "\n",
    "        # Try to get the image size (width, height) using PIL\n",
    "        try:\n",
    "            img_shape = Image.open(img_path).size\n",
    "        except Exception as e:\n",
    "            print(f\"Error opening image {img_path}: {e}\")\n",
    "            continue  # Skip this file if there is an error opening the image\n",
    "\n",
    "        # Convert the annotation from XML to YOLO format\n",
    "        yolo_format = convert_to_yolo_format(xml_path, img_shape)\n",
    "\n",
    "        # Save the YOLO formatted annotation to a text file\n",
    "        yolo_label_path = os.path.join(yolo_labels_dir, xml_file.replace('.xml', '.txt'))\n",
    "        try:\n",
    "            with open(yolo_label_path, 'w') as f:\n",
    "                f.writelines(yolo_format)\n",
    "            print(f\"Processed: {xml_file}\")\n",
    "        except Exception as e:\n",
    "            print(f\"Error saving YOLO file {yolo_label_path}: {e}\")\n"
